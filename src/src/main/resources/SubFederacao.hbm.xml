<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="modelos.SubFederacao" table="dados_subfederacoes">
        <meta attribute="class-description"> 
   
        </meta>
        <id column="id" name="id" type="int">
            <generator class="sequence">
                <param name="sequence">dados_subfederacoes_id_seq</param>
            </generator>  
        </id>
        <property name="nome" not-null="true" type="string"/>
        <property name="descricao" type="string"/>
        <property column="data_ultima_atualizacao" name="ultimaAtualizacao" type="timestamp"/>
        <property name="url" not-null="true" type="string"/>
        <property column="data_xml" name="dataXML" type="string"/>
    
    <set name="repositorios" table="repositorios_subfed" cascade="all" inverse="true">
      <key column="id_subfed"/>
      <one-to-many class="modelos.RepositorioSubFed"/>
    </set>

    </class>
</hibernate-mapping>
