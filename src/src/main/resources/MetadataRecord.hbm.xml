<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="modelos.PadraoMetadados" table="padraometadados">
    <id column="id" name="id" type="int">
      <meta attribute="scope-set">protected</meta>
      <generator class="native"/>
    </id>
    <property name="nome" type="string"/>
    <property name="metadataPrefix" type="string" column="metadata_prefix"/>
    <property name="namespace" type="string" column="name_space"/>
    
    <!--
    <set name="repositorios" table="repositorios">
      <key column="padrao_metadados"/>
      <one-to-many class="modelos.Repositorio"/>
    </set>
    -->
  </class>
</hibernate-mapping>
