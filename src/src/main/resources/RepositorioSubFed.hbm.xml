<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="modelos.RepositorioSubFed" table="repositorios_subfed">
    
        <id column="id" name="id" type="int">
            <meta attribute="scope-set">protected</meta>
            <generator class="native"/>
        </id>
        <property name="nome" not-null="true" type="string"/>
        
        <many-to-one class="modelos.SubFederacao" column="id_subfed" name="subFederacao" not-null="true"/>    
        
       <set name="documentos" table="documentos" cascade="all-delete-orphan">
      <key column="id_rep_subfed"/>
      <one-to-many class="modelos.DocumentoReal"/>
    </set>

    </class>
</hibernate-mapping>
