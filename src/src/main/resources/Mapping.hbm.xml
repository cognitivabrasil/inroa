<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="modelos.Mapeamento" table="mapeamentos">
    <id column="id" name="id" type="int">
      <meta attribute="scope-set">protected</meta>
      <generator class="native"/>
    </id>
    
    <property name="description" type="string" column="descricao"/>
    <property name="name" type="string" column="nome"/>
    <property name="xslt" type="string" />
    <many-to-one class="modelos.PadraoMetadados" column="padrao_id" name="padraoMetadados"/>
    
    
  </class>
</hibernate-mapping>
