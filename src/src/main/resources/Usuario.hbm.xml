<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="feb.data.entities.Usuario" table="usuarios">

		<id column="id" name="id" type="int">
			<generator class="sequence">
				<param name="sequence">usuarios_id_seq</param>
			</generator>
		</id>
		<property name="login" not-null="true" type="string" />
		<property name="passwordMd5" type="string" column="senha" />
		<property name="description" type="string" column="nome" />
		<property name="role" type="string"/>
		<property name="permissionsInternal" type="string" column="permissions" />

	</class>
</hibernate-mapping>
