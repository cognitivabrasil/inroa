<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.cognitivabrasil.feb.data.entities.Mapeamento" table="mapeamentos">
    <id column="id" name="id" type="int">
      <generator class="sequence">
        <param name="sequence">mapeamentos_id_seq</param>
      </generator>
    </id>
    <property column="descricao" name="description" type="string"/>
    <property column="nome" name="name" type="string"/>
    <property name="xslt" type="string"/>
    <property column="xml_exemplo" name="xmlSample" type="string"/>
    <many-to-one class="com.cognitivabrasil.feb.data.entities.PadraoMetadados" column="padrao_id" fetch="join" name="padraoMetadados"/>
  </class>
</hibernate-mapping>
