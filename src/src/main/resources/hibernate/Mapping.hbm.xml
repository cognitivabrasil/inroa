<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="feb.data.entities.Mapeamento" table="mapeamentos">
        <id column="id" name="id" type="int">
            <generator class="sequence">
                <param name="sequence">mapeamentos_id_seq</param>
            </generator>  
        </id>
    <property name="description" type="string" column="descricao"/>
    <property name="name" type="string" column="nome"/>
    <property name="xslt" type="string" />
    <property name="xmlSample" column="xml_exemplo" type="string" />
    <many-to-one class="feb.data.entities.PadraoMetadados" column="padrao_id" name="padraoMetadados" fetch="join"/>
    
    
  </class>
</hibernate-mapping>
