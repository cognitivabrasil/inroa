<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.cognitivabrasil.feb.data.entities.Document" table="documentos">
    <id column="id" name="id" type="int">
      <generator class="seqhilo">
        <param name="sequence">documentos_id_seq</param>
        <param name="max-size">50</param>
      </generator>
    </id>
    <property column="obaa_entry" name="obaaEntry" not-null="true" type="string"/>
    <property column="obaaxml" name="obaaXml" type="string"/>
    <many-to-one class="com.cognitivabrasil.feb.data.entities.Repositorio" column="id_repositorio" name="repositorio"/>
    <many-to-one class="com.cognitivabrasil.feb.data.entities.RepositorioSubFed" column="id_rep_subfed" name="repositorioSubFed"/>
    <property column="created" name="created" type="timestamp"/>
    <property name="deleted" type="boolean"/>
  </class>
</hibernate-mapping>
