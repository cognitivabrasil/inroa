<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="feb.data.entities.DocumentoReal" table="documentos">
        <id column="id" name="id" type="int">
            <generator class="seqhilo">
                <param name="sequence">documentos_id_seq</param>
                <param name="max-size">50</param>
            </generator>
        </id>
        <property column="obaa_entry" name="obaaEntry" not-null="true"
                          type="string" />
        <property column="obaaxml" name="obaaXml" type="string" />
        <many-to-one class="feb.data.entities.Repositorio" column="id_repositorio"
                             name="repositorio" />
        <many-to-one class="feb.data.entities.RepositorioSubFed" column="id_rep_subfed"
                             name="repositorioSubFed" />

        <property name="timestamp" type="timestamp" />
        <set name="objetos" cascade="all-delete-orphan" table="objetos"
                     inverse="true" order-by="atributo,valor asc">
            <key column="documento" />
            <one-to-many class="feb.data.entities.Objeto" />
        </set>
        <property name="deleted" type="boolean" />

        <set name="autores" cascade="all-delete-orphan" table="autores" order-by="nome asc"
                     inverse="true">
            <key column="documento" />
            <one-to-many class="feb.data.entities.Autor" />
        </set>

        <set name="tokens" cascade="all-delete-orphan" table="r1tokens"
                     inverse="true">
            <key column="documento_id" />
            <one-to-many class="feb.data.entities.Token" />
        </set>
    </class>
</hibernate-mapping>