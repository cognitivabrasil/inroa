<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.cognitivabrasil.feb.data.entities.Autor" table="autores">
    <id column="id" name="id" type="int">
      <generator class="seqhilo">
        <param name="sequence">autores_id_seq</param>
        <param name="max-size">100</param>
      </generator>
    </id>
    <property column="nome" name="nome" type="string"/>
    <many-to-one class="com.cognitivabrasil.feb.data.entities.Document" column="documento" name="doc"/>
  </class>
</hibernate-mapping>
