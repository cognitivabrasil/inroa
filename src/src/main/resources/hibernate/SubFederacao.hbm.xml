<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.cognitivabrasil.feb.data.entities.SubFederacao" table="dados_subfederacoes">
    <meta attribute="class-description">

		</meta>
    <id column="id" name="id" type="int">
      <generator class="sequence">
        <param name="sequence">dados_subfederacoes_id_seq</param>
      </generator>
    </id>
    <property column="nome" name="name" not-null="true" type="string"/>
    <property name="descricao" type="string"/>
    <property column="data_ultima_atualizacao" name="ultimaAtualizacao" type="timestamp"/>
    <property name="url" not-null="true" type="string"/>
    <property column="data_xml" name="dataXML" type="string"/>
    <property name="version" type="string"/>
    <set cascade="save-update" fetch="join" inverse="true" name="repositorios" order-by="name asc" table="repositorios_subfed">
      <key column="id_subfed"/>
      <one-to-many class="com.cognitivabrasil.feb.data.entities.RepositorioSubFed"/>
    </set>
  </class>
</hibernate-mapping>
