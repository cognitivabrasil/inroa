<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.cognitivabrasil.feb.data.entities.Repositorio" table="repositorios">
    <meta attribute="class-description"> 
   
        </meta>
    <id column="id" name="id" type="int">
      <generator class="sequence">
        <param name="sequence">repositorios_id_seq</param>
      </generator>
    </id>
    <property column="nome" name="name" not-null="true" type="string"/>
    <property name="descricao" type="string"/>
    <property column="url_or_ip" name="url" not-null="true" type="string"/>
    <property column="metadata_prefix" name="metadataPrefix" not-null="true" type="string"/>
    <property column="data_ultima_atualizacao" name="ultimaAtualizacao" type="timestamp"/>
    <property column="name_space" name="namespace" type="string"/>
    <property column="periodicidade_horas" name="periodicidadeAtualizacao" type="integer"/>
    <property column="internal_set" name="colecoesInternal" type="string"/>
    <property column="data_xml" name="dataOrigem" type="timestamp"/>
    <many-to-one class="com.cognitivabrasil.feb.data.entities.PadraoMetadados" column="padrao_metadados" fetch="join" name="padraoMetadados"/>
    <many-to-one class="com.cognitivabrasil.feb.data.entities.Mapeamento" column="mapeamento_id" fetch="join" name="mapeamento"/>
    <set cascade="save-update" inverse="true" name="documentos" table="documentos">
      <key column="id_repositorio"/>
      <one-to-many class="com.cognitivabrasil.feb.data.entities.DocumentoReal"/>
    </set>
  </class>
</hibernate-mapping>
