<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.cognitivabrasil.feb.data.entities.Usuario" table="usuarios">
    <id column="id" name="id" type="int">
      <generator class="sequence">
        <param name="sequence">usuarios_id_seq</param>
      </generator>
    </id>
    <property name="username" column="login" not-null="true" type="string"/>
    <property column="senha" name="passwordMd5" type="string"/>
    <property column="nome" name="description" type="string"/>
    <property name="role" type="string"/>
    <property column="permissions" name="permissionsInternal" type="string"/>
  </class>
</hibernate-mapping>
