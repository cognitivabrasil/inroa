<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="feb.data.entities.Objeto" table="objetos">
    <meta attribute="class-description"> 
    </meta>
 <id column="id" name="id" type="int">
        <generator class="sequence">
                <param name="sequence">objetos_id_seq</param>
        </generator>    </id>
    <property name="atributo" not-null="true" type="string"/>
    <property name="valor" type="string">
      <meta attribute="field-description">When the track was created</meta>
    </property>
    
    <many-to-one name="documento" class="feb.data.entities.DocumentoReal" fetch="select">
            <column name="documento" not-null="true" />
    </many-to-one>
  </class>
</hibernate-mapping>
